<h1>Listing places</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Creator</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Range</th>
    <th>Public</th>
    <th>Home</th>
    <th>Work</th>
    <th>Gmaps</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @places.each do |place| %>
  <tr>
    <td><%= place.name %></td>
    <td><%= place.address %></td>
    <td><%= place.creator_id %></td>
    <td><%= place.latitude %></td>
    <td><%= place.longitude %></td>
    <td><%= place.range %></td>
    <td><%= place.public %></td>
    <td><%= place.home %></td>
    <td><%= place.work %></td>
    <td><%= place.gmaps %></td>
    <td><%= link_to 'Show', place %></td>
    <td><%= link_to 'Edit', edit_place_path(place) %></td>
    <td><%= link_to 'Destroy', place, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Place', new_place_path %>
