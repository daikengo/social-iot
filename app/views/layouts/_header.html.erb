<div id="options">
	<% if current_user %>		
		<span class="action"> <%= link_to t(:signout), logout_path, :method => 'delete' %></span>	
	<% else %>
		<span class="action">
			<%= link_to t(:signup), new_user_path %>
		</span>
		<span class="action"> <%= link_to t(:signin), login_path %></span>
	<% end %>
</div>

<div id="logo"><img src="/images/logo.png" height="69"/></div>
